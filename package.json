{
  "name": "vue-fomantic-ui",
  "version": "0.18.0",
  "description": "Fomantic-UI component library for Vue.js 3.0",
  "license": "MIT",
  "bugs": "https://github.com/nightswinger/vue-fomantic-ui/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/nightswinger/vue-fomantic-ui"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue-fomantic-ui.umd.js",
  "module": "./dist/vue-fomantic-ui.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-fomantic-ui.mjs",
      "require": "./dist/vue-fomantic-ui.umd.js"
    }
  },
  "keywords": [
    "semantic-ui",
    "fomantic-ui",
    "vue"
  ],
  "scripts": {
    "dev": "vite",
    "dev:storybook": "storybook dev",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.2",
    "clsx": "^1.1.1",
    "motion": "^10.17.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.2",
    "@storybook/addon-actions": "^7.6.17",
    "@storybook/addon-controls": "^7.6.17",
    "@storybook/vue3-vite": "^7.6.17",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-vue": "^9.22.0",
    "fomantic-ui-css": "^2.9.0",
    "happy-dom": "^13.3.8",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.10",
    "typescript": "^5.4.2",
    "vite": "^5.1.5",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1",
    "vue": "~3.4.21",
    "vue-tsc": "^2.0.6"
  }
}
